{
    "name": "cyjs",
    "version": "0.4.1",
    "description": "A simple C-c C-v tool in command line.",
    "files": [
        "bin",
        "lib"
    ],
    "bin": {
        "cy": "bin/cy.js"
    },
    "scripts": {
        "build": "babel src -d lib --extensions \".ts\"",
        "clean": "rimraf lib",
        "lint": "eslint --fix src/**/*.ts",
        "prepare": "husky install",
        "prepublishOnly": "npm run tsc && npm run clean && npm run build",
        "release": "standard-version",
        "start": "babel -w src -d lib --extensions \".ts\"",
        "tsc": "tsc --noEmit"
    },
    "author": "yytcjcy@gmail.com",
    "license": "MIT",
    "dependencies": {
        "cac": "5.0.16",
        "chalk": "4.1.2",
        "fs-extra": "10.1.0"
    },
    "devDependencies": {
        "@babel/cli": "7.17.10",
        "@babel/core": "7.18.2",
        "@babel/plugin-proposal-class-properties": "7.17.12",
        "@babel/preset-env": "7.18.2",
        "@babel/preset-typescript": "7.17.12",
        "@commitlint/cli": "17.0.2",
        "@commitlint/config-conventional": "17.0.2",
        "@types/fs-extra": "9.0.13",
        "@types/node": "17.0.35",
        "@yotsubacy/config": "1.5.0",
        "eslint": "8.16.0",
        "husky": "8.0.1",
        "lint-staged": "13.0.0",
        "rimraf": "3.0.2",
        "standard-version": "9.5.0",
        "typescript": "4.7.2"
    }
}
