{
    "name": "cyjs",
    "version": "0.2.1",
    "description": "A simple C-c C-v tool in command line.",
    "files": [
        "bin",
        "lib"
    ],
    "bin": {
        "cy": "./bin/cy.js"
    },
    "scripts": {
        "start": "babel -w src -d lib",
        "build": "babel src -d lib",
        "clean": "rimraf lib",
        "prepublishOnly": "npm run clean && npm run build"
    },
    "author": "yytcjcy@gmail.com",
    "license": "MIT",
    "dependencies": {
        "cac": "^4.4.4",
        "chalk": "^4.0.0",
        "fs-extra": "^4.0.3"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "rimraf": "^3.0.2"
    },
    "babel": {
        "presets": [
            [
                "env",
                {
                    "targets": {
                        "node": "4"
                    },
                    "useBuiltIns": false
                }
            ]
        ]
    }
}
