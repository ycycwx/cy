{
    "name": "cyjs",
    "version": "0.4.1",
    "description": "A simple C-c C-v tool in command line.",
    "files": [
        "bin",
        "lib"
    ],
    "bin": {
        "cy": "bin/cy.js"
    },
    "scripts": {
        "build": "babel src -d lib --extensions \".ts\"",
        "clean": "rimraf lib",
        "lint": "eslint --fix src/**/*.ts",
        "prepare": "husky",
        "prepublishOnly": "npm run tsc && npm run clean && npm run build",
        "release": "standard-version",
        "start": "babel -w src -d lib --extensions \".ts\"",
        "tsc": "tsc --noEmit"
    },
    "author": "yytcjcy@gmail.com",
    "license": "MIT",
    "dependencies": {
        "cac": "^6.7.14",
        "chalk": "^4.1.2",
        "fs-extra": "^10.1.0"
    },
    "devDependencies": {
        "@babel/cli": "7.28.6",
        "@babel/core": "7.28.6",
        "@babel/plugin-transform-class-properties": "7.28.6",
        "@babel/preset-env": "7.28.6",
        "@babel/preset-typescript": "7.28.5",
        "@commitlint/cli": "20.4.0",
        "@commitlint/config-conventional": "20.4.0",
        "@types/fs-extra": "9.0.13",
        "@types/node": "22.19.2",
        "@yotsubacy/config": "2.3.5",
        "eslint": "9.39.2",
        "husky": "9.1.7",
        "lint-staged": "16.2.7",
        "rimraf": "6.1.2",
        "standard-version": "9.5.0",
        "typescript": "5.9.3"
    }
}
